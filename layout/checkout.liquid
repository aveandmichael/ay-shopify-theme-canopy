            <script>
                  (function($){
                   $( document ).ready( function(){
              var bc_len = document.querySelectorAll( 'ol.breadcrumb li' ).length;
                     if( bc_len > 0 ){
                    var curr_index = 0;
                    var curr_index1 = 0;
                    document.querySelectorAll( 'ol.breadcrumb li' ).forEach( function(e,i){
                      if( document.querySelectorAll( 'ol.breadcrumb li' )[i].classList.contains( 'breadcrumb__item--current' ) ) {
                        //document.querySelectorAll( 'ul.bc li' )[i].classList.add( 'bc_current' );
                        curr_index1 = i;
                      }
                      curr_index = i;
                    } );

                    for( var i=0; i<bc_len; i++ ) {
                      if( i<curr_index1 ) {
                        document.querySelectorAll( 'ul.bc li' )[i].classList.remove( 'bc_current' );
                        document.querySelectorAll( 'ul.bc li' )[i].classList.add( 'bc_complete' );
                      }
                      if( i==curr_index1 ) {
                        document.querySelectorAll( 'ul.bc li' )[i].classList.add( 'bc_current' );
                      }
                    }
                     } else {
                       $( 'ul.bc li' ).addClass( 'bc_complete' );
                       $( 'ul.bc li' ).filter(':last').removeClass( 'bc_complete' ).addClass( 'bc_current' );
                     }
                  } );
                  })(Checkout.$)
            </script>
            <ul class="bc">
