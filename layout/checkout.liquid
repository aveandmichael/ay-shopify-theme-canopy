    <style>
      .section.section--shipping-address .section__header {
        display: flex;
      }
      .section.section--shipping-address .section__header h2 {
        flex: 50%;
        padding: 15px;
        background: #f8f8f8;
        border-radius: 6px 6px 0px 0px;
        font-weight: bold;
        font-size: 14px;
      }
      .section.section--shipping-address .section__header {
        margin-bottom: 0px;
      }
      .section.section--shipping-address .section__header h2:not(.active) {
        border-bottom: solid 1px #d5d5d5;
      }
      .section.section--shipping-address .section__header h2:not(.active):hover {

        cursor: pointer !important;
        background: #eaeaea;
      }
      .section.section--shipping-address .section__header h2.active {
        background: #fff;
        border-top: solid 1px #d5d5d5;
        border-left: solid 1px #d5d5d5;
        border-right: solid 1px #d5d5d5;
        border-bottom: solid 1px #fff;
      }
      .section.section--shipping-address .section__content {
        border-left: solid 1px #d5d5d5;
        border-right: solid 1px #d5d5d5;
        border-bottom: solid 1px #d5d5d5;
        border-radius: 0px 0px 6px 6px;
        padding: 10px;
      }

      .section.section--shipping-address .section__content .fieldset .address-fields {
        display: flex;
        flex-wrap: wrap;
      }
      [data-address-field="first_name"] {
        order: 1;
      }
      [data-address-field="last_name"] {
        order: 2;
      }
      [data-address-field="address1"] {
        order: 3;
      }
      [data-address-field="address2"] {
        order: 4;
      }
      [data-address-field="zip"] {
        order: 5;
      }
      [data-address-field="city"] {
        order: 6;
      }
      [data-address-field="country"] {
        order: 7;
      }
    </style>
    <script>
      ( function($) {
        $( '.section.section--shipping-address .section__header #section-delivery-title' ).addClass( 'active' );
        $( '.section.section--shipping-address .section__header' ).append( '<h2 class="section__title" id="section-packstation-title"> {{ 'custom_translations.checkout_page.deliver_dhl' | t }} </h2>' );
        //$( '[data-address-field="address1"]' ).after( '<div style="display: none;" data-address-field="packstation" data-autocomplete-field-container="true" class="field field--required field--show-floating-label"><div class="field__input-wrapper"><label class="field__label field__label--visible" for="checkout_shipping_address_packstation">Packstation number</label><input placeholder="Packstation number" autocomplete="shipping packstation-number" autocorrect="off" role="combobox" aria-autocomplete="list" aria-expanded="false" aria-required="true" data-backup="packstation" data-autocomplete-trigger="true" data-autocomplete-title="Suggestions" class="field__input" size="30" type="text" value="" name="checkout[shipping_address][packstation]" id="checkout_shipping_address_packstation" aria-haspopup="false"></div></div>' );
        $( '.section.section--shipping-address .section__header h2' ).on( 'click', function(){
          if( $( this ).hasClass( 'active' ) ) {
            return;
          }
          var $title = $( this ).attr( 'id' );
          console.log( $title );
          $( '.section.section--shipping-address .section__header h2' ).removeClass( 'active' );
          $( this ).addClass( 'active' );
          switch( $title ) {
            case 'section-delivery-title':
              $( '[data-address-field="address1"] label' ).html( '{{ 'custom_translations.checkout_page.deliver_st_1' | t }}' );
              $( '[data-address-field="address1"] input' ).attr( 'placeholder', '{{ 'custom_translations.checkout_page.deliver_st_1' | t }}' );
              $( '[data-address-field="address2"] label' ).html( '{{ 'custom_translations.checkout_page.deliver_add_1' | t }}' );
              $( '[data-address-field="address2"] input' ).attr( 'placeholder', '{{ 'custom_translations.checkout_page.deliver_add_1' | t }}' );
              break;
            case 'section-packstation-title':
              $( '[data-address-field="address1"] label' ).html( '{{ 'custom_translations.checkout_page.deliver_st_2' | t }}' );
              $( '[data-address-field="address1"] input' ).attr( 'placeholder', '{{ 'custom_translations.checkout_page.deliver_st_2' | t }}' );
              $( '[data-address-field="address2"] label' ).html( '{{ 'custom_translations.checkout_page.deliver_add_2' | t }}' );
              $( '[data-address-field="address2"] input' ).attr( 'placeholder', '{{ 'custom_translations.checkout_page.deliver_add_2' | t }}' );
              break;
          }
        } );
      } )( Checkout.$ );
    </script>
    <!-- end of custom code -->

