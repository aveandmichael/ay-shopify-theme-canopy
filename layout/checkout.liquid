      (function($) {
        $(document).on("page:load page:change", async () => {
          var summaryTable = $(".product-table");
          if (summaryTable) {
            var items = summaryTable.find("tbody tr");
            if (items && items.length > 0) {
              items.each((i, o) => {
                var variantTitle = $(o).find("span.product__description__variant");
                if (variantTitle) {
                  var variantTitleSplit = variantTitle.text().toString().split("/");
                  if (variantTitleSplit && variantTitleSplit.length > 1) {
                    variantTitle.html(`${variantTitleSplit[0]} ${variantTitleSplit[1]}`)
                  }
                }
              })
            }
          }
        })
      })(Checkout.$);






















