                <li class="bc_item bc_complete">{{ 'custom_translations.checkout_page.breadcrumb_1' | t | newline_to_br }}</li>
              {% endcomment %}
              <li class="bc_item bc_current">{{ 'custom_translations.checkout_page.breadcrumb_2' | t }}</li>
              <li class="bc_item">{{ 'custom_translations.checkout_page.breadcrumb_3' | t | newline_to_br }}</li>
              <li class="bc_item">{{ 'custom_translations.checkout_page.breadcrumb_4' | t | newline_to_br }}</li>
