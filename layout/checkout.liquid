    <script type="text/javascript">
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      
      (function($) {
        // generic functions
        function setLocale(window) {
          let windowLocation;
          if (window) {
            windowLocation = window.location;
            if (windowLocation.toString().includes("/en/")) {
              return "en"
            }
            return "de"
          }
        }

        // functions for contact information step
        var clearAllFieldErrorMessages = function() {
          $("p.field__message--error").remove()
          $("div[data-address-field]").removeClass("field--error")
        }
        var clearPhoneFieldValue = function (phoneContainer) {
          phoneField = phoneContainer.find("#checkout_shipping_address_phone")
          phoneField.val(null)
        }
        var togglePhoneField = function(countryField, phoneContainer) {
          if (countryField.val() === "Germany") {
            phoneContainer.hide()
          } else {
            phoneContainer.show()
          }
        }
        var changePhoneFieldText = function(phoneContainer) {
          phoneField = phoneContainer.find("#checkout_shipping_address_phone")
          phoneLabel = phoneContainer.find(".field__label")
          phoneField.attr("placeholder", "{{ 'custom_translations.checkout_page.deliver_phone' | t }}")
          phoneLabel.text("{{ 'custom_translations.checkout_page.deliver_phone' | t }}")
        }
        var changeFirstNameFieldText = function(firstNameField) {
          firstNameLabel = firstNameField.find(".field__label")
          firstNameField.attr("placeholder", "{{ 'custom_translations.checkout_page.deliver_fn' | t }}")
          firstNameLabel.text("{{ 'custom_translations.checkout_page.deliver_fn' | t }}")
        }

        // functions for order summary section
        function hideRecurringTotalText() {
          const recurringTotalText = $("#order-summary .total-line--recurring-total")
          if (recurringTotalText) {
            recurringTotalText.hide()
          }
        }
        async function translateSellingPlanText(location) {
          try {
            const sellingPlanText = $("#order-summary .product__description__selling_plan")
            if (sellingPlanText) {
              if (location === 'DE' || location === 'AT') {
                const translation = await fetch("https://translation.googleapis.com/language/translate/v2?key=AIzaSyDf_slvJS-odkZNVxTond60vtpjdfW2glI", {
                  "method": "POST",
                  "body": JSON.stringify({
                    "q": `${sellingPlanText.text()}`,
                    "source": "en",
                    "target": "de",
                    "format": "text"
                  })
                }).then((res) => res.json())
                if (translation && translation.data && translation.data.translations && translation.data.translations.length > 0) {
                  sellingPlanText.text(translation.data.translations[0].translatedText)
                }
              }
            }
          } catch (e) {
            console.log("ERROR", e.message)
          }
        }
        async function translateVariant(location) {
          try {
            const orderSummaryLineItems = $("#order-summary .order-summary__sections .product-table tbody")
            if (orderSummaryLineItems) {
              await orderSummaryLineItems.children().each(async (i, e) => {
                if (e) {
                  const variantText = $(e).find(".product__description__variant.order-summary__small-text")
                  if (variantText) {
                    if (location === 'DE' || location === 'AT') {
                      const translation = await fetch("https://translation.googleapis.com/language/translate/v2?key=AIzaSyDf_slvJS-odkZNVxTond60vtpjdfW2glI", {
                        "method": "POST",
                        "body": JSON.stringify({
                          "q": `${variantText.text()}`,
                          "source": "en",
                          "target": "de",
                          "format": "text"
                        })
                      }).then((res) => res.json())
                      if (translation && translation.data && translation.data.translations && translation.data.translations.length > 0) {
                        variantText.text(translation.data.translations[0].translatedText)
                        console.log(JSON.stringify(translation.data.translations[0]))
                      }
                    }
                  }
                }
              })
            }
          } catch (e) {
            console.log("ERROR TRANSLATION", e.message)
          }
        }
        async function detectCountryCode() {
          await $.getJSON('https://apiip.net/api/check?accessKey=b9372a73-cc79-4ac6-b56f-c9dbc660b5ec&output=json&fields=countryCode', function(result) {
            window.sessionStorage.setItem('visitorGeolocationCountryCode', result.countryCode);
          })
        }
        async function translateOrderSummary() {
          //await $.getJSON('https://apiip.net/api/check?accessKey=b9372a73-cc79-4ac6-b56f-c9dbc660b5ec&output=json&fields=countryCode', function(result) {
          //  translateSellingPlanText(result && result.countryCode)
          //  translateVariant(result && result.countryCode)
          //})
          const countryCode = window.sessionStorage.getItem('visitorGeolocationCountryCode');
          if (countryCode) {
            translateSellingPlanText(countryCode)
            translateVariant(countryCode)
          }
        }
        function hideShippingOptionDeliveryText() {
          const deliveryText = $(".section__delivery-option-group .radio__label__accessory .content-box__small-text")
          if (deliveryText) {
            deliveryText.hide()
          }
        }
        function hideProductDescPropertyText() {
          const prodDescPropText = $("#order-summary  .product__description__property")
          if (prodDescPropText) {
            prodDescPropText.hide()
          }
        }
        function reduceInputFieldsContactInformationStep() {
          const loginInput = document.querySelector('.section--contact-information div.section__header #main-header + p.layout-flex__item');
          const marketingInputCheck = document.querySelector('.section__content .fieldset-description #checkout_buyer_accepts_marketing');
          const marketingInputText = document.querySelector('.section__content .fieldset-description label.checkbox__label[for="checkout_buyer_accepts_marketing"]');
          //const shippingAddress2Input = document.querySelector('#checkout_shipping_address_address2');
          if (loginInput) {
            loginInput.setAttribute('hidden', true);
          }
          if (marketingInputCheck) {
            marketingInputCheck.style = "display: none !important";
          }
          if (marketingInputText) {
            marketingInputText.style = "display: none !important";
          }
          // if (shippingAddress2Input) {
          //   shippingAddress2Input.style = "display: none !important";
          // }
        }
        function disableOrHideDHLOption() {
          const packStationOptionEl = document.getElementById("section-packstation-title");
          const countryCode = window.sessionStorage.getItem('visitorGeolocationCountryCode');
          if (packStationOptionEl && countryCode && (countryCode!=='DE' && countryCode!=='AT' && countryCode!=='CH')) {
            packStationOptionEl.setAttribute('hidden', true)
          }
        }
        function hideShowPhoneField(countryDropdown, phoneFieldContainer) {
          const packStationOptionEl = document.getElementById("section-packstation-title");
          if (countryDropdown.value === 'Germany') {
            phoneFieldContainer.style.display = 'none';
            if (packStationOptionEl) {
              packStationOptionEl.removeAttribute('hidden')
            }
          } else {
            phoneFieldContainer.style.display = 'block';
            if (packStationOptionEl) {
              packStationOptionEl.setAttribute('hidden', true)
            }
          }
        }
        function returnToPdp(doc) {
          if (Shopify.Checkout.step === 'contact_information') {
            console.log("IS CONTACT INFO");
            let rtcEl = document.querySelector(".step__footer .step__footer__previous-link");
            if (rtcEl) {
              console.log("IS RTCEL");
              rtcEl.href = doc.referrer;
              if (doc.referrer && doc.referrer.includes("products/product-detail") && doc.referrer.includes("skinid") && doc.referrer.includes("token")) {
                console.log("IS DOC REFERRER");
                let rtcElText = rtcEl.querySelector(".step__footer__previous-link-content");
                if (rtcElText) {
                  console.log("IS RTCELTEXT");
                  rtcElText.innerText = "{{ 'custom_translations.checkout_page.return_to_pdp' | t }}";
                }
              }
            }
          }
        }
        function hideSubscriptionPolicy() {
          let policyEl = document.querySelector('[data-modal="policy-subscription-policy"]');
          if (policyEl) {
            policyEl.style.display = 'none';
          }
        }
        function gtmTrackShippingMethod() {
          let smOptEls = document.querySelectorAll(".section--shipping-method .input-radio");
          console.log("adawdadadawd", smOptEls)
          if (smOptEls) {
            smOptEls.forEach((el) => {
              el.addEventListener("click", () => {
                if (el && el.checked) {
                  console.log("TESTSETESTETETS")
                  console.log({{ checkout.line_items | json}})
                  console.log(Checkout)
                  console.log(Shopify)
                }
              });
            });
          }
        }

        $(document).on("page:load page:change", async () => {
          console.log("DOCUMENT REFERRER", document.referrer);
          // things to do on contact information
          if (Shopify.Checkout.step === 'contact_information') {
            var countryField = $('#checkout_shipping_address_country')
            var phoneFieldContainer = document.querySelector("div[data-address-field=phone]")
            var phoneContainer = $("[data-address-field=phone]")
            var phoneField = $('#checkout_shipping_address_phone')
            var phoneTooltip = $('#phone_tooltip')
            var phoneLabel = $("[data-address-field=phone] .field__label")
            var firstNameField = $('#checkout_shipping_address_first_name')
  
            // await togglePhoneField(countryField, phoneContainer)
            // await changePhoneFieldText(phoneContainer)
            // await changeFirstNameFieldText(firstNameField)

            // hiding/showing phone field based on
            // selected shipping country
            var countryDropdown = document.querySelector('#checkout_shipping_address_country')
            var phoneFieldContainer = document.querySelector("div[data-address-field=phone]")
            var phoneFieldQuestionIconContainer = phoneFieldContainer.querySelector('.field__icon')
            phoneFieldQuestionIconContainer.parentElement.style.position = 'relative'
            hideShowPhoneField(countryDropdown, phoneFieldContainer); // executes when pages loaded
            countryDropdown.addEventListener('change', function(e) { // executes when dropdown changed
              hideShowPhoneField(countryDropdown, phoneFieldContainer);
            });

            reduceInputFieldsContactInformationStep()
            await detectCountryCode()
            disableOrHideDHLOption()
          }

          // things to do on order summary
          $(document).ready(() => {
            returnToPdp(document);
            hideSubscriptionPolicy();
            hideRecurringTotalText()
            hideShippingOptionDeliveryText()
            translateOrderSummary()
            hideProductDescPropertyText()
            gtmTrackShippingMethod()
          })
        })
    })(Checkout.$);






















    </script>

