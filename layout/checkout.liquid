            <style>
              .bc {
                margin: 15px 0px 0px 0px;
                padding: 0px;
                list-style-type: none;
                width: 100%;
                max-width: 800px;
                height: 120px;
                counter-reset: breadcrumb;
                position: relative;
              }
              .bc._1:before {
                background: linear-gradient(90deg, #278b0088 calc(20% + 10%), #aaaaaa88 calc(20% + 10%)) !important;
              }
              .bc._2:before {
                background: linear-gradient(90deg, #278b0088 calc(40% + 10%), #aaaaaa88 calc(40% + 10%)) !important;
              }
              .bc._3:before {
                background: linear-gradient(90deg, #278b0088 calc(60% + 10%), #aaaaaa88 calc(60% + 10%)) !important;
              }
              .bc._4:before {
                background: linear-gradient(90deg, #278b0088 calc(80% + 10%), #aaaaaa88 calc(80% + 10%)) !important;
              }
              .bc._5:before {
                background: linear-gradient(90deg, #278b0088 100%, #aaaaaa88 100%) !important;
              }
              .bc:before {
                position: absolute;
                top: 23px;
                left: 0%;
                content: '';
                width: 100%;
                height: 2px;
                background: #278b00;
              }
              .bc .bc_item {
                float: left;
                padding-top: 60px;
                width: 33.33%;
                text-align: center;
                position: relative;
              }
              /*.bc .bc_item:nth-of-type(1):after {
                content: url( 'https://cdn.shopify.com/s/files/1/0663/2783/8964/files/sock.svg' ) !important;
              }*/
              /*.bc .bc_item.bc_complete:not(:nth-of-type(1)):after {*/

              .bc .bc_item:after {
                counter-increment: breadcrumb;
                content: counter(breadcrumb);
                position: absolute;
                left: calc(50% - 15px);
                width: 30px;
                height: 30px;
                line-height: 30px;
                text-align: center;
                border-radius: 50%;
                background: #323232;
                color: #fff;
              }
              .bc .bc_item.bc_complete:after {
                content: '\2713';
              }
              .bc .bc_item:after,
              .bc .bc_current:after {
                top: 9px;
              }

              .bc .bc_complete:after {
                transform: scale(1.3);
              }

              .bc .bc_complete:after,
              .bc .bc_current:after{
                background: #278b00;
                color: #fff;
              }

              .breadcrumb {
                display: none;
              }
            </style>
