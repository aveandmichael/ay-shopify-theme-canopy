  function prepareLifestyleHtml(headerTitle, img, val){
    var str = "";
    str += '<article class="skin-section__body-item show" data-notation="skinType">';
    str += '<div class="skin-section__body-item-icon"><img src="'+img+'" alt=""></div>';
    str += '<input type="hidden" value="Dry">'
    str +=  '<h2 class="h6 skin-section__body-item-title text-left">'+headerTitle+'</h2>'
    str +=  '<div class="skin-section__body-item-iwr">'
    str +=  ' <div class="skin-section__body-item-text show">'                         
    str +=   '<span>'+val[0].description+'</span>'                           
    str +=   '<input type="hidden" value="<-Dry->">';                      
    str +=   ' </div>';                      
    str +=   '</div>';            
    str +=    '</article>' ;        
    return str;
  }


   
  function skinTypeInfo(val, lifestyleType){
    var typedata = lifestyleAllData.filter(function(item){
     return item.type === lifestyleType;
  })

  if( typedata.length !== 0){
  let skinTypeSel = typedata[0].skinType.filter(function(item){
     return item.type === val;
  })
   if (skinTypeSel.length !== 0) return prepareLifestyleHtml(typedata[0].title, typedata[0].img, skinTypeSel); else return "";
  }else{
    return "";
  }
  }


  function fetchLifestyleData(){
    let skinIdCustLife= getUrlVars()["skinid"];
     let tokenLife = getUrlVars()["token"];
     $.ajax({
  type: "POST",
  url: "https://europe-west1-metrics-production-1.cloudfunctions.net/getAnswers/"+skinIdCustLife+"/"+tokenLife,
  success: function(response) {
   console.log(response)
    // $("#slideLifestyle").append("")
    let skinConditionType = response.skinCondition.skinType;
    let reddeningType = response.skinCondition.reddening;
    let flyingFreqType = response.lifestyle.flyingFrequency;
    let sunExpositionType = response.lifestyle.sunExposition;
    let glassWaterType = response.lifestyle.glassWaterIntake;
    let weeklySports = response.lifestyle.weeklySports;
    let stressLevelType = response.lifestyle.stressLevel;
    let smokingType = response.lifestyle.smoking;
    let blueLightType = response.lifestyle.bluelightExposition;
    let urbanEnvironmentType = response.lifestyle.urbanEnvironment;
    let sleepType = response.lifestyle.sleep;
    let nutritionType = response.lifestyle.nutrition.sort();
    let weeklySportsType = response.lifestyle.weeklySports.sort();

    
    let skinInfo =  skinTypeInfo(skinConditionType, "skintype");
    if(typeof reddeningType !== "undefined")  skinInfo +=  skinTypeInfo(reddeningType, "reddening");
    
    skinInfo +=  skinTypeInfo(flyingFreqType, "flyingFrequency");
    skinInfo +=  skinTypeInfo(sunExpositionType, "sunExposition");
    skinInfo +=  skinTypeInfo(glassWaterType, "glassWaterIntake");
    skinInfo +=  skinTypeInfo(stressLevelType, "stressLevel");
    skinInfo +=  skinTypeInfo(smokingType, "smoking");

    skinInfo +=  skinTypeInfo(blueLightType, "bluelightExposition");
    skinInfo +=  skinTypeInfo(urbanEnvironmentType, "urbanEnvironment");

    skinInfo +=  skinTypeInfo(sleepType, "sleep");
    skinInfo +=  skinTypeInfo(nutritionType.toString(), "nutrition");
    skinInfo +=  skinTypeInfo(weeklySportsType.toString(), "weeklySports");
    
    
    $("#slideLifestyle").append(skinInfo);
  }

  });
  }
  
  
